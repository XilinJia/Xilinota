{
  "name": "xilinota-webclipper-popup",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@babel/eslint-parser": "7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "camelcase": "8.0.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "^6.8.1",
    "dotenv": "16.4.4",
    "dotenv-expand": "10.0.0",
    "file-loader": "6.2.0",
    "fs-extra": "11.2.0",
    "html-webpack-plugin": "5.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "mini-css-extract-plugin": "2.7.6",
    "pnp-webpack-plugin": "1.7.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-safe-parser": "^7.0.0",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "redux": "5.0.1",
    "resolve": "1.22.8",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "13.3.3",
    "semver": "^7.5.4",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-pnp": "1.2.0",
    "url": "^0.11.3",
    "url-loader": "4.1.1",
    "webpack": "5.89.0",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "7.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "echo 'Clipper build is disabled due to ERR_OSSL_EVP_UNSUPPORTED error on CI'",
    "build_DISABLED": "node scripts/build.js SKIP_PREFLIGHT_CHECK",
    "test": "node scripts/test.js --env=jsdom",
    "watch": "cra-build-watch",
    "postinstall": "node postinstall.js && npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "9.1.3",
    "cra-build-watch": "^3.4.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fs-extra": "11.2.0",
    "md5": "^2.3.0",
    "react-scripts": "^5.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}